{{#if isBooked}}
  <div class="as-calendar-occurrence__container1">
  {{#if hasBlock}}
    {{yield}}
  {{else}}
    <h1 class="as-calendar-occurrence__title" style={{titleStyle}}>{{title}}</h1>
    <p> {{startingTime}} - {{dayEndingTime}}</p>
  {{/if}}

  {{#unless isInteracting}}
    {{#if isRemovable}}
      <a {{action "remove" bubbles=false}} class="as-calendar-occurrence__remove"></a>
    {{/if}}

    {{#if isResizable}}
      <div class="as-calendar-occurrence__resize-handle"></div>
    {{/if}}
  {{/unless}}
  </div>
{{else}}
  <div class="as-calendar-occurrence__container">
  {{#if hasBlock}}
    {{yield}}
  {{else}}
    <h1 class="as-calendar-occurrence__title" style={{titleStyle}}>{{title}}</h1>
    <p> {{startingTime}} - {{dayEndingTime}}</p>
  {{/if}}

  {{#unless isInteracting}}
    {{#if isRemovable}}
      <a {{action "remove" bubbles=false}} class="as-calendar-occurrence__remove"></a>
    {{/if}}

    {{#if isResizable}}
      <div class="as-calendar-occurrence__resize-handle"></div>
    {{/if}}
  {{/unless}}
  </div>
{{/if}}
